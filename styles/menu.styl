.menu
	z-index 1
	overflow hidden
	position relative
	max-width 946px
	height 70px
	background #474747
	margin 0 auto
	
	.geo-pin
		position relative
		//float left
		background url(../images/map_pin.png) no-repeat
		height 17px
		width 12px
		z-index 2
		top 20px
		left 36px

	

.menu-right
	z-index 1
	position relative
	float right
	width 30%
	.item-right-select
		float right
		width 200px
		select
			z-index 1
			height 24px
			width 198px
			position relative
			text-align-last center
			font-family "NexaBold"
			font-size 11px
			background #474747
			color #fff
			border none
			-webkit-appearance none
			border-right 1px solid #767676
			border-left 1px solid #767676
			top 3px
			
		p
			position absolute
			color #fff
			font-size 12px
			z-index 3
			text-align center
			cursor pointer
			-webkit-transform rotate(90deg)
			-moz-transform rotate(90deg)
			font-family "Quicksand"
			padding 6px 177px
	.item-right-a
		.geo-pin-a
			position absolute
			//float left
			background url(../images/map_pin.png) no-repeat
			height 17px
			width 12px
			z-index 2
			top 5px
			left 65px
		a
			position relative
			text-decoration none
			color #fff
			font-family "NexaBold"
			font-size 12px
			top 4px
.wrapper-right
	margin 0 auto
	position absolute
	float right
	width 100%
	height 70px
	left 30%
	color #fff
	background #474747
	top 65px
.menu-item
		z-index 1
		height 70px
		overflow hidden
		position relative
		float left
		left 84px
		top 5px
		.item
			float left
			height 16px
			width 80px
			margin-right 20px
			border-right 1px solid #767676
			cursor pointer
			p
				position relative
				font-family "NexaBold"
				font-size 12px
				color #ffffff
				padding-right 20px
		.border
				border none 
				