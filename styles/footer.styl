@font-face {
    font-family "Helvetica"  
    src url("../fonts/Helvetica/Helvetica-Regular.woff2") format("woff2")
    src url("../fonts/Helvetica/Helvetica-Regular.woff") format("woff")
}


.footer
	z-index 1
	overflow hidden
	position relative
	max-width 946px
	height 65px
	background #474747
	margin 0 auto
	margin-bottom 55px
.menu-item.for-footer
	float right
	left 0px
	top 28px
.footer-right
	margin 0 auto
	position absolute
	float right
	width 100%
	height 65px
	left 30%
	color #fff
	background #474747
	top 0px
	
.footer-wrapper
	position relative
	width 100%
	height 487px
	background #373737
.footer-menu
	position relative
	width 400px
	height 15px
	.footer-menu-item
		position relative
		float left
		font-size 12px
		color #fff
		font-family "Helvetica"  
		margin-right 20px
		margin-bottom 25px
.footer-text
	position relative
	display inline-block
	font-size 11px
	color #7e7e7e
	font-family "Helvetica"  
	margin-right 225px
.wrapper-footer-menu
	position relative
	overflow hidden
	margin 0 auto
	max-width 946px
.wrapper-item.border
	width 50px
	height 30px
	border-bottom 6px solid #f26c4f !important